			<hookname>gts_whos_online</hookname>
			<phpcode><![CDATA[if(file_exists(DIR . '/includes/functions_gteamspeak.php'))
{
	require_once(DIR . '/includes/functions_gteamspeak.php');
}

$pl = $cyts->info_playerList();

$gteamspeak_total_online = count($pl);
$gteamspeak_total_active = 0;

if ($pl && $gteamspeak_total_online > '0')
{

	foreach ($pl AS $gtsuser)
	{
		$gtsuser['style'] = "";

		list($gtsuser['gtsname'], $gtsuser['userid']) = explode(".", $gtsuser['loginname']);

		$gtsuser['nick'] = htmlentities($gtsuser['nick']);

		$gtsuser['lastactivedate'] = vbdate($vbulletin->options['dateformat'], (TIMENOW - $gtsuser['idletime']), 1);
		$gtsuser['lastactivetime'] = vbdate($vbulletin->options['timeformat'], (TIMENOW - $gtsuser['idletime']));

		$gtsuser['loggedindate'] = vbdate($vbulletin->options['dateformat'], (TIMENOW - $gtsuser['logintime']), 1);
		$gtsuser['loggedintime'] = vbdate($vbulletin->options['timeformat'], (TIMENOW - $gtsuser['logintime']));

		$p_flags = $cyts->info_translateFlag($gtsuser['pflags'], 3);
		$p_privs = $cyts->info_translateFlag($gtsuser['pprivs'], 2);

		if ($p_flags["AW"])
		{
			$gtsuser['style'] .= $vbulletin->options['gts_whos_online_flag_aw'];
		}

		if ($p_privs["CA"] || $p_privs["SA"])
		{
			$gtsuser['style'] .= $vbulletin->options['gts_whos_online_flag_sa_ca'];
		}

		if ($p_privs["SA"])
		{
			$gtsuser['style'] .= $vbulletin->options['gts_whos_online_flag_sa'];
		}

		if ($vbulletin->options['gts_whos_online_display_idle'] == '1')
		{
			eval('$gteamspeak_activeusers .= ", ' . fetch_template('gts_whos_online_bit') . '";');
		}
		elseif ($gtsuser['idletime'] < ($vbulletin->options['gts_whos_online_timeout'] * 60))
		{
			eval('$gteamspeak_activeusers .= ", ' . fetch_template('gts_whos_online_bit') . '";');
		}

		if ($gtsuser['idletime'] < ($vbulletin->options['gts_whos_online_timeout'] * 60) && !$p_flags["AW"])
		{
			$gteamspeak_total_active++;
		}
	}

	$gteamspeak_total_inactive = $gteamspeak_total_online - $gteamspeak_total_active;

	$gteamspeak_activeusers = substr($gteamspeak_activeusers, 2);

}]]></phpcode>
